{
  "name": "react-sail",
  "version": "0.5.0",
  "author": "vdfor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vdfor/react-sail.git"
  },
  "bugs": {
    "url": "https://github.com/vdfor/react-sail/issues"
  },
  "homepage": "https://github.com/vdfor/react-sail#readme",
  "proxy": {
    "/api": {
      "target": "http://localhost:3000",
      "secure": "false"
    }
  },
  "private": true,
  "dependencies": {
    "antd": "^3.2.1",
    "axios": "^0.18.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-onerror": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "log4js": "^2.5.3",
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.2",
    "pm2": "^2.10.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0"
  },
  "scripts": {
    "start": "cross-env PORT=8182 SSR=off react-app-rewired start --scripts-version react-scripts-ts",
    "build": "npm run clean && cross-env PUBLIC_URL=/ SSR=off react-app-rewired build --scripts-version react-scripts-ts && mv build dist",
    "start:server": "cross-env NODE_ENV=development nodemon --exec ts-node --project ./server/tsconfig.json ./server/src/bootstrap.ts",
    "build:ssr": "npm run clean && mkdir -p dist/server && npm run build:client && mv build dist/client && npm run build:server && mv server/build dist/server/build && cp -r server/config dist/server",
    "build:client": "cross-env PUBLIC_URL=/ SSR=on react-app-rewired build --scripts-version react-scripts-ts",
    "build:server": "webpack --config ./server/webpack.config.js --progress --color",
    "server:dev": "cross-env NODE_ENV=development node ./dist/server/build/index.js",
    "prod": "NODE_ENV=production pm2 start --name react-sail ./dist/server/build/index.js",
    "clean": "rm -rf dist/",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/koa": "^2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-compress": "^2.0.8",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.0.27",
    "@types/koa-static": "^4.0.0",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-loadable": "^5.3.3",
    "@types/react-router-dom": "^4.2.4",
    "awesome-typescript-loader": "^3.5.0",
    "cross-env": "^5.1.3",
    "externals-dependencies": "^1.0.4",
    "nodemon": "^1.17.1",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.4.1",
    "ts-import-plugin": "^1.4.3",
    "typescript": "^2.7.2"
  }
}
